export ZSH="$HOME/.oh-my-zsh"
export ZSH_CUSTOM="$HOME/.dotfiles/oh-my-zsh/zsh_custom"
plugins=(
  gitfast
  git-auto-fetch
  docker
  direnv
  terraform
  tmux
  fzf
  autojump
  aliases
  exports
  asdf
  custom-kubectl
)
ZSH_THEME="sorin"
HYPHEN_INSENSITIVE="true"
